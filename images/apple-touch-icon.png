<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 180 180" width="180" height="180">
  <!-- Background circle -->
  <circle cx="90" cy="90" r="85" fill="#8B5CF6" stroke="#6D28D9" stroke-width="5"/>
  
  <!-- Musical note -->
  <g fill="white">
    <!-- Note head -->
    <ellipse cx="67" cy="124" rx="17" ry="14" transform="rotate(-20 67 124)"/>
    <!-- Note stem -->
    <rect x="81" y="56" width="8" height="68" rx="4"/>
    <!-- Note flag -->
    <path d="M89 56 Q112 45 124 68 Q112 78 89 73 Z"/>
    <!-- Additional decorative element -->
    <circle cx="124" cy="112" r="8"/>
    <rect x="119" y="90" width="10" height="22" rx="5"/>
    <!-- Second smaller note -->
    <ellipse cx="45" cy="100" rx="8" ry="6" transform="rotate(-20 45 100)"/>
    <rect x="52" y="80" width="4" height="20" rx="2"/>
  </g>
  
  <!-- Subtle highlight -->
  <circle cx="62" cy="62" r="12" fill="rgba(255,255,255,0.3)"/>
  <!-- Musical staff lines (subtle) -->
  <g stroke="rgba(255,255,255,0.2)" stroke-width="2" fill="none">
    <line x1="30" y1="140" x2="150" y2="140"/>
    <line x1="30" y1="130" x2="150" y2="130"/>
    <line x1="30" y1="120" x2="150" y2="120"/>
  </g>
</svg>

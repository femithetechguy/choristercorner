<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 630" width="1200" height="630">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B5CF6;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#A855F7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7C3AED;stop-opacity:1" />
    </linearGradient>
    <filter id="textShadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="2" dy="4" stdDeviation="3" flood-color="#000" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <rect width="1200" height="630" fill="url(#bgGrad)"/>
  
  <!-- Musical note illustration -->
  <g fill="white" opacity="0.9">
    <!-- Main large note -->
    <ellipse cx="200" cy="450" rx="60" ry="50" transform="rotate(-20 200 450)"/>
    <rect x="250" y="250" width="30" height="200" rx="15"/>
    <path d="M280 250 Q360 220 400 290 Q360 320 280 305 Z"/>
    
    <!-- Secondary notes -->
    <ellipse cx="120" cy="380" rx="30" ry="25" transform="rotate(-20 120 380)"/>
    <rect x="145" y="320" width="18" height="60" rx="9"/>
    
    <circle cx="400" cy="400" r="30"/>
    <rect x="385" y="320" width="35" height="80" rx="17"/>
    
    <!-- Decorative treble clef -->
    <path d="M500 320 Q540 280 570 340 Q540 400 500 380 Q525 340 500 320" 
          stroke="white" stroke-width="12" fill="none" opacity="0.7"/>
  </g>
  
  <!-- Text content -->
  <g fill="white" filter="url(#textShadow)">
    <text x="650" y="280" font-family="Arial, sans-serif" font-size="72" font-weight="bold">ChoristerCorner</text>
    <text x="650" y="340" font-family="Arial, sans-serif" font-size="32" opacity="0.9">Worship Songs, Chords & Resources</text>
    <text x="650" y="380" font-family="Arial, sans-serif" font-size="28" opacity="0.8">for Church Musicians</text>
    
    <!-- Feature highlights -->
    <text x="650" y="460" font-family="Arial, sans-serif" font-size="24" opacity="0.7">✓ Song Library with Lyrics</text>
    <text x="650" y="490" font-family="Arial, sans-serif" font-size="24" opacity="0.7">✓ YouTube Integration</text>
    <text x="650" y="520" font-family="Arial, sans-serif" font-size="24" opacity="0.7">✓ Mobile-Friendly Experience</text>
  </g>
  
  <!-- Decorative elements -->
  <g fill="rgba(255,255,255,0.1)">
    <circle cx="1000" cy="150" r="40"/>
    <circle cx="1100" cy="500" r="30"/>
    <circle cx="100" cy="150" r="25"/>
  </g>
  
  <!-- Musical staff lines (subtle) -->
  <g stroke="rgba(255,255,255,0.15)" stroke-width="3" fill="none">
    <line x1="50" y1="500" x2="550" y2="500"/>
    <line x1="50" y1="480" x2="550" y2="480"/>
    <line x1="50" y1="460" x2="550" y2="460"/>
    <line x1="50" y1="440" x2="550" y2="440"/>
    <line x1="50" y1="420" x2="550" y2="420"/>
  </g>
</svg>
